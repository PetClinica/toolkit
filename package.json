{
  "name": "toolkit",
  "version": "1.0.0",
  "description": "Typescript toolkit",
  "private": true,
  "workspaces": [
    "discord",
    "logger",
    "test",
    "trace"
  ],
  "scripts": {
    "build": "npm run clean && tsc -b",
    "clean": "tsc -b --clean",
    "test": "vitest run --root ./",
    "test:watch": "vitest --root ./"
  },
  "dependencies": {
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.4",
    "vitest": "^3.0.4"
  }
}
